---
import {SEO} from "astro-seo";
import { ViewTransitions } from 'astro:transitions';
const { 
        title = 'N Sri Meher Chaitanya', 
        description = 'ML Intern@GE Vernova, Student at University of Hyderabad', 
        imageUrl = "/seo/seo-main.png" 
} = Astro.props
---

<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width" />

<SEO 
  title={title}
  description={description}
  openGraph={{
    basic: {
      title: title + "'s Website",
      type: "website",
      image: imageUrl,
    },
    optional: {
      description: description,
    },
    image: {
      url: imageUrl,
      type: "image/jpeg",
      alt: "N Sri Meher Chaitanya"
    }
  }}
  twitter={{
    card: "summary_large_image",
    site: "@smc181002",
    creator: "@smc181002",
  }}
/>

<!-- <link rel="icon" type="image/x-icon" href="/assets/logo.svg" /> -->
<link rel="icon" href="/favicon.svg" type="image/svg+xml">
<link rel="preconnect" href="https://fonts.gstatic.com" />
<link rel="preload" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

<meta name="generator" content={Astro.generator} />
